Shader "Sprite/MaskOutside" {
    Properties {        
    }

    SubShader {
        Tags { "Queue"="Transparent" }

        Pass {
            Blend SrcAlpha OneMinusSrcAlpha
            ZWrite Off
            Cull Off  
			
            Stencil {
                Ref 1
				WriteMask 0
                Comp greater
				Pass keep
				Fail keep
				ZFail keep
            }

   

            CGPROGRAM

            struct a2v {
                float4 pos : POSITION;
            };

            struct v2f {
                float4 pos : SV_POSITION;
            };

            v2f vert(a2v i) {
                v2f o;
                o.pos = mul(uMVPMatrix, i.pos);
                return o;
            }

            float4 frag(v2f i) : SV_TARGET {
                return float4(1, 1, 1, 1);
            }

            ENDCG
        }
    }
}