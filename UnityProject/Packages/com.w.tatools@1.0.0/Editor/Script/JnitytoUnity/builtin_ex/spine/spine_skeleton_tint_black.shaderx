Shader "hidden/Spine/Skeleton TintBlack" {
    Properties {
        _uMainTex ( "main texture", 2D ) = "" {}
    }
    SubShader {
        Tags {"Queue"="Transparent"}
        Pass {
            Blend SrcAlpha OneMinusSrcAlpha
            ZTest Less
            ZWrite Off
            Cull Off

            CGPROGRAM
            

            uniform sampler2D _uMainTex;

            struct appdata_t {
                float4 pos : POSITION;
                float2 uv : TEXCOORD0;
                float4 color : COLOR0;
                float4 color2 : COLOR1;
            };
            struct v2f {
                float4 pos : SV_POSITION;
                float2 uv : TEXCOORD0;
                float4 color : COLOR0;
                float4 color2 : COLOR1;
            };

            v2f vert (appdata_t v) {
                v2f o;
                o.pos = mul(uMVPMatrix, v.pos);
                o.uv = v.uv;
                o.color = v.color;
                o.color2 = v.color2;
                return o;
            }

            float4 frag(v2f i) : SV_TARGET {
                float4 texColor = tex2D(_uMainTex, i.uv);
                float alpha = texColor.a * i.color.a;
                float4 color;
                color.a = alpha;
                color.rgb = (1.0 - texColor.rgb) * i.color2.rgb * alpha + texColor.rgb * i.color.rgb;
                return color;
            }

            ENDCG
        }
    }
}